{
  "name": "horton-properties-backend",
  "version": "1.0.0",
  "description": "Backend API for Horton Properties Data Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:log": "node server.js 2>&1 | tee -a ../logs/dev-server.log",
    "dev": "nodemon server.js",
    "dev:log": "NODE_OPTIONS=\"-r ../log-hook.js\" nodemon server.js",
    "test": "jest",
    "test:ingest": "vitest run",
    "ingest:clean": "ts-node scripts/testIngest.ts ./test_cashflow_clean.csv"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "pg": "^8.16.3",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "express",
    "mysql",
    "csv",
    "api",
    "property-management"
  ],
  "author": "Alex Horton",
  "license": "MIT"
}
